{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589",
  "scripts": {
    "clean": "pnpm -r exec -- rm -rf node_modules",
    "version:major": "pnpm version major",
    "version:minor": "pnpm version minor",
    "version:patch": "pnpm version patch",
    "publish": "pnpm publish -r",
    "postinstall": "pnpm run prettier:postinstall",
    "prettier:postinstall": "prettier --write '**/package.json' '**/pnpm-lock.yaml'",
    "check": "pnpm run check:lint && pnpm run check:prettier && pnpm run check:types",
    "check:lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "check:prettier": "prettier --check --ignore-path .gitignore .",
    "check:types": "pnpm -r exec -- npm run check:types",
    "autofix": "pnpm run autofix:lint && pnpm run autofix:prettier",
    "autofix:lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "autofix:prettier": "prettier --write --ignore-path .gitignore .",
    "build:local": "pnpm -r run build:local",
    "build": "pnpm -r run build",
    "build:skipLocales": "pnpm -r run build:js",
    "build:ci": "pnpm -r run build:ci",
    "build:watch": "pnpm -r run build:watch --parallel",
    "start:gui": "pnpm run start --filter packages/gui",
    "dev:gui": "pnpm run dev --filter @chia-network/gui",
    "electron": "pnpm run start:gui",
    "locale:extract": "pnpm -r run locale:extract",
    "locale:clean": "pnpm -r run locale:clean",
    "locale": "pnpm -r run locale",
    "audit": "pnpm -r exec -- npm audit",
    "audit:fix": "pnpm -r exec -- npm audit fix",
    "test": "pnpm -r run test --stream --passWithNoTests --coverage --coverageDirectory=.coverage && pnpm run coverage:merge && pnpm run coverage:report",
    "coverage:merge": "npx istanbul-merge --out .nyc_output/coverage.json .coverage",
    "coverage:report": "npx nyc report --reporter=text --reporter=lcov",
    "restoreMessages": "node restoreMessages.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint",
    "*": "prettier --check --ignore-unknown --ignore-path .gitignore"
  },
  "dependencies": {
    "core-js": "3.32.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-typescript": "7.22.5",
    "@types/jest": "29.5.3",
    "@types/lodash": "4.14.197",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "date-and-time": "2.4.3",
    "eslint": "8.47.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "istanbul-merge": "2.0.0",
    "jest": "29.6.3",
    "lint-staged": "14.0.1",
    "nyc": "15.1.0",
    "prettier": "3.1.0",
    "typescript": "5.1.6"
  },
  "version": "1.2.10-dev132"
}
