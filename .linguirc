{
  "catalogs": [{
    "path": "<rootDir>/src/locales/{locale}/messages",
    "include": ["<rootDir>/src"],
    "exclude": ["**/node_modules/**"]
  }],
  "locales": ["da", "de", "en", "es", "fi", "fr", "it", "ja", "pt", "ro", "ru", "sk", "sv", "zh", "zh-CN"],
  "format": "po",
  "sourceLocale": "en",
  "fallbackLocales": {
    default: "en"
  }
}
