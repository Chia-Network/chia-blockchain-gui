{
  "catalogs": [{
    "path": "<rootDir>/src/locales/{locale}/messages",
    "include": ["<rootDir>/src"],
    "exclude": ["**/node_modules/**"]
  }],
  "locales": ["da-DK", "de-DE", "en-AU", "en-PT", "en-US", "es-ES", "fi-FI", "fr-FR", "it-IT", "ja-JP", "nl-NL", "pl-PL", "pt-BR", "pt-PT", "ro-RO", "ru-RU", "sk-SK", "sv-SE", "zh-TW", "zh-CN"],
  "format": "po",
  "sourceLocale": "en-US",
  "fallbackLocales": {
    default: "en-US"
  }
}
