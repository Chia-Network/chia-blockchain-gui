{
  "catalogs": [{
    "path": "<rootDir>/src/locales/{locale}/messages",
    "include": ["<rootDir>/src"],
    "exclude": ["**/node_modules/**"]
  }],
  "locales": ["ar-SA", "bg-BG", "cs-CZ", "da-DK", "de-DE", "el-GR", "en-AU", "en-NZ", "en-PT", "en-US", "es-ES", "es-AR", "es-MX", "fa-IR", "fi-FI", "fr-FR", "hr-HR", "id-ID", "it-IT", "ja-JP", "ko-KR", "nl-NL", "no-NO", "pl-PL", "pt-BR", "pt-PT", "ro-RO", "ru-RU", "sk-SK", "sr-SP", "sv-SE", "tr-TR", "uk-UA", "zh-TW", "zh-CN"],
  "format": "po",
  "sourceLocale": "en-US",
  "fallbackLocales": {
    default: "en-US"
  }
}
