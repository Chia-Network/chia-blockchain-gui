{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    ["@babel/preset-react", {
      "runtime": "automatic"
    }]
  ],
  "plugins": [
    "macros",
    "@babel/plugin-transform-runtime",
    "babel-plugin-styled-components",
    ["babel-plugin-transform-imports", {
      "@material-ui/core": {
        "transform": "@material-ui/core/${member}",
        "preventFullImport": true
      },
      "@material-ui/icons": {
        "transform": "@material-ui/icons/${member}",
        "preventFullImport": true
      }
    }]
  ]
}
