{
  "overrides": [
    {
      "files": ["*.yaml", "*.yml", "*.toml", "*.json", "*.ini"],
      "options": {
        "tabWidth": 2,
        "singleQuote": false,
        "experimentalTernaries": true,
        "useTabs": false
      }
    },
    {
      "file": ["*.js", "*.ts"],
      "options": {
        "printWidth": 120,
        "singleQuote": true,
      }
    }
  ]
}
