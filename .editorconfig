root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
#max_line_length = 120

[{src/assets/**,src/locales/**/*.js}]
indent_size = unset
end_of_line = unset
insert_final_newline = unset
trim_trailing_whitespace = unset
